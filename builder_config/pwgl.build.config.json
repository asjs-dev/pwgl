{
  "js": {
    "files": ["./src/index.js"],
    "lzw": false,
    "wrapped": true,
    "minify": true,
    "compress": true,
    "variables": {
      "{{appVersion}}": "json.version",
      "{{date}}": "date"
    },
    "replaceTexts": {
      "removeFromArray": "$rfa",
      "arraySet": "$as",
      "noop": "$nf",
      "noopReturnsOne": "$nfro",

      "BaseTransformProps": "DBTP",
      "ColorProps": "DCP",
      "DistortionProps": "DDP",
      "FilterTextureTransformProps": "DFTTP",
      "ItemTransformProps": "DITP",
      "LightTransformProps": "DLTP",
      "TextureCrop": "DTC",
      "TextureTransformProps": "DTTP",
      "TextureInfo": "DTI",
      "StageContainer": "DSC",
      "BaseFilter": "FBF",
      "BaseKernelFilter": "FBKF",
      "BaseSamplingFilter": "FBSF",
      "BaseTextureFilter": "FBTF",
      "Matrix3Utilities": "GM3U"
    },
    "output": "dist/pwgl.min.js"
  },
  "copyAssets": {
    "./dist/pwgl.min.js": "./docs/assets/pwgl.latest.min.js"
  }
}
