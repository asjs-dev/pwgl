{
  "js": {
    "packageJson": "package.json",
    "files": ["./src/index.js"],
    "lzw": false,
    "wrapped": true,
    "minify": true,
    "compress": true,
    "variables": {
      "{{appVersion}}": "json.version",
      "{{date}}": "date"
    },
    "replaceTexts": {
      "removeFromArray": "$rfa",
      "arraySet": "$as",
      "noop": "$nf",
      "noopReturnsWith": "$nfrw",
      "BASE_VERTEX_SHADER": "BVS",
      "BASE_VERTEX_SHADER_POSITION": "BVSP",
      "BASE_VERTEX_SHADER_ATTRIBUTES": "BVSA",
      "BASE_VERTEX_SHADER_UNIFORMS": "BVSU",
      "BASE_VERTEX_SHADER_INITIALIZATION": "BVSI",
      "TINT_TYPE_SHADER": "TTS",
      "CREATE_SAMPLING_FILTER": "CSF",

      "BaseTransform": "DBT",
      "Color": "DC",
      "Distortion": "DD",
      "FilterTextureTransform": "DFT",
      "ItemTransform": "DIT",
      "LightTransform": "DLT",
      "TextureCrop": "DTC",
      "TextureTransform": "DTT",
      "TextureInfo": "DTI",
      "StageContainer": "DSC",
      "Matrix3Utilities": "GM3"
    },
    "output": "dist/pwgl.min.js"
  },
  "copyAssets": {
    "./dist/pwgl.min.js": "./docs/assets/pwgl.min.js",
    "./dist/pwgl.umd.min.js": "./docs/assets/pwgl.umd.min.js",
    "./dist/pwgl.es.min.js": "./docs/assets/pwgl.es.min.js"
  }
}
