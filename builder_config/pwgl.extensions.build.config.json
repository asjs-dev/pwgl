{
  "js": {
    "packageJson": "extensions/package.json",
    "files": ["./extensions/index.js"],
    "lzw": false,
    "wrapped": true,
    "minify": true,
    "compress": true,
    "variables": {
      "{{appVersion}}": "json.version"
    },
    "replaceTexts": {
      "BaseAudio": "ABA"
    },
    "output": "dist/pwgl.extensions.min.js"
  },
  "copyAssets": {
    "./dist/pwgl.extensions.min.js": "./docs/assets/pwgl.extensions.min.js",
    "./dist/pwgl.extensions.umd.min.js": "./docs/assets/pwgl.extensions.umd.min.js",
    "./dist/pwgl.extensions.es.min.js": "./docs/assets/pwgl.extensions.es.min.js"
  }
}
